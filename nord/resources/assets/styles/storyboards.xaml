<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Storyboard x:Key="FadeInStory" x:ConnectionId="1" Name="FadeInStory">
    <DoubleAnimation From="0" To="0" Duration="00:00:0.2" BeginTime="00:00:00" Storyboard.TargetProperty="Opacity" />
    <DoubleAnimation From="0" To="1" Duration="00:00:0.3" BeginTime="00:00:0.2" Storyboard.TargetProperty="Opacity" />
  </Storyboard>
  <Storyboard x:Key="FadeOutStory" x:ConnectionId="2" Name="FadeOutStory">
    <DoubleAnimation From="1" To="0" Duration="00:00:0.2" BeginTime="00:00:00" Storyboard.TargetProperty="Opacity" />
  </Storyboard>
  <Storyboard x:Key="FadeInActiveBackgroundStory">
    <ColorAnimation To="#F8F8F9" Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.3" />
  </Storyboard>
</ResourceDictionary>